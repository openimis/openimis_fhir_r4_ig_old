{
    "resourceType": "StructureDefinition",
    "url": "http://localhost:8080/fhir/StructureDefinition/Claim",
    "name": "Claim",
    "status": "draft",
    "fhirVersion": "4.0.1",
    "kind": "resource",
    "abstract": false,
    "type": "Claim",
    "title": "Claim - Profile",
    "baseDefinition": "http://hl7.org/fhir/StructureDefinition/Claim",
    "derivation": "constraint",
    "differential": {
        "element": [
            {
                "id": "Claim.identifier",
                "path": "Claim.identifier",
                "comment": "Claim can have multiple identifiers. The most important one is mapped from ClaimUUID."
            },
            {
                "id": "Claim.status",
                "path": "Claim.status",
                "comment": "Has the entered state by default",
                "short": "Entered | Checked | Reviewed | Valuated",
                "type": [
                    {
                        "code": "code"
                    }
                ],
                "binding": {
                    "strength": "required",
                    "valueSet": "http://localhost:8080/fhir/ValueSet/vs-claimstatus"
                },
                "isSummary": true,
                "isModifier": true,
                "defaultValueCode": "Entered"
            },
            {
                "id": "Claim.patient",
                "path": "Claim.patient",
                "binding": {
                    "strength": "required"
                }
            },
            {
                "id": "Claim.billablePeriod",
                "path": "Claim.billablePeriod",
                "comment": "DateFrom and DateTo are mapped to a period of time between them"
            },
            {
                "id": "Claim.diagnosis",
                "path": "Claim.diagnosis"
            },
            {
                "id": "Claim.provider",
                "path": "Claim.provider"
            },
            {
                "id": "Claim.facility",
                "path": "Claim.facility",
                "type": [
                    {
                        "code": "Reference",
                        "targetProfile": [
                            "http://localhost:8080/fhir/StructureDefinition/Practitioner"
                        ]
                    }
                ]
            },
            {
                "id": "Claim.item.category.text",
                "path": "Claim.item.category.text",
                "base": {
                    "path": "Claim.item.category.text",
                    "min": 0,
                    "max": "1"
                },
                "type": [
                    {
                        "code": "string"
                    }
                ]
            },
            {
                "id": "Claim.item.extension.productOrServiceReference",
                "path": "Claim.item.extension.productOrServiceReference",
                "definition": "Reference to Medication resource if it is a medical item, reference to ActivityDefinition resource if it is medical service",
                "base": {
                    "path": "Claim.item.extension.productOrServiceReference",
                    "min": 0,
                    "max": "1"
                },
                "type": [
                    {
                        "code": "Reference",
                        "targetProfile": [
                            "http://localhost:8080/fhir/StructureDefinition/Medication",
                            "http://localhost:8080/fhir/StructureDefinition/ActivityDefinition"
                        ]
                    }
                ]
            }
        ]
    }
}